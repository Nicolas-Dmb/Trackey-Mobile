{
  "expo": {
    "name": "Trackey",
    "slug": "trackey",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FCFDFA"
    },
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "l'accès à la camera permettra à l'application de scanner les qrcodes et de prendre les photos de vos clés."
        }
      ]
    ],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "6",
      "bundleIdentifier": "com.trackey.trackey",
      "infoPlist": {
        "NSCameraUsageDescription": "l'accès à la camera permettra à l'application de scanner les qrcodes et de prendre les photos de vos clés.",
        "NSPhotoLibraryUsageDescription":"l'accès à vos photos permettra de séléctionner les photos de vos clés."
      },
      "splash": {
        "image": "./assets/splash-ios.png",
        "resizeMode": "contain",
        "backgroundColor": "#FCFDFA"
      }
    },
    "android": {
      "package": "com.trackey.trackey",
      "versionCode": 4,
      "permissions": [
        "CAMERA",
        "android.permission.CAMERA"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FCFDFA"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "ed818dd9-f990-4ac0-b09e-723776352885"
      }
    },
    "platforms": [
      "ios",
      "android"
    ],
    "owner": "nick3.0"
  },
  "configVersion": 0,
  "apple": {
    "info": {
      "en-US": {
        "title": "Trackey",
        "subtitle": "Gestion des clés pour syndic",
        "description": "Logiciel de gestion des clés pour les syndics de copropriétés",
        "keywords": [
          "Trackey",
          "app"
        ],
        "privacyPolicyUrl": "https://trackey.fr/Donnees",
        "supportUrl": "https://trackey.fr/contact"
      }
    }
  },
  "build": {
    "production": {
      "android": {
        "workflow": "managed"
      }
    }
  }
}
  
